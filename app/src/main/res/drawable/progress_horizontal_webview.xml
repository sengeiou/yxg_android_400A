<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:startColor="#ffe1e1e1"
                android:endColor="#ffe1e1e1"
                android:angle="270.0"
                android:centerY="0.75"
                android:centerColor="#ffe1e1e1" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient
                    android:startColor="#ff3f739a"
                    android:endColor="#ff3f739a"
                    android:angle="270.0"
                    android:centerY="0.75"
                    android:centerColor="#ff3f739a" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#ff8D3D1A"
                    android:endColor="#ff8D3D1A"
                    android:angle="270.0"
                    android:centerY="0.75"
                    android:centerColor="#ff8D3D1A" />
            </shape>
        </clip>
    </item>
</layer-list>